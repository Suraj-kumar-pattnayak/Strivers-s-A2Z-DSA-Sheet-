class Solution {
  public:
    // arr : given array
    // k : find kth smallest element and return using this function
    int kthSmallest(vector<int> &nums, int k) {
        priority_queue <int> maxh;
        for(int i=0; i<nums.size(); i++)
        {
            maxh.push(nums[i]);
            if(maxh.size() > k) maxh.pop();
        }
        return maxh.top();
    }
};
